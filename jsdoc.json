{
  "plugins": [],
  "recurseDepth": 10,
  "source": {
    "include": ["./dist/"],
    "includePattern": ".js$",
    "excludePattern": "(node_modules/docs)"
  },
  "opts": {
    "recurse": "true",
    "destination": "./doc",
    "template": "./node_modules/@pixi/jsdoc-template"
  },
  "templates": {
    "applicationName": "Utilities Library",
    "disqus": "",
    "googleAnalytics": "",
    "openGraph": {
      "title": "Utilities Library Docs",
      "type": "website",
      "image": "",
      "site_name": "",
      "url": ""
    },
    "meta": {
      "title": "Utilities Library",
      "description": "library to manage vectors, matrices, numbers and strings",
      "keyword": "Library"
    },
    "linenums": true,
    "source": {
      "include": ["./dist/"],
      "includePattern": ".+\\.js(doc)?$",
      "excludePattern": "(^|\\/|\\\\)_"
    },
    "opts": {
      "encoding": "utf8",
      "recurse": true,
      "private": false,
      "lenient": true,
      "destination": "./docs",
      "template": "./node_modules/@pixi/jsdoc-template"
    }
  },
  "pixi": {
    "styles": ["path/to/custom/styles.css"],
    "scripts": ["path/to/custom/script.js"]
  }
}
